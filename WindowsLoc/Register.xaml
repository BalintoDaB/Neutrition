<Window x:Class="Neutrition.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Neutrition"
        mc:Ignorable="d"
        Background="{StaticResource Primary}"
        FontFamily="{StaticResource RobotoFont}"
        FontSize="20"
        Title="Register" Height="510" Width="300">
    <Grid>
        <Grid.Resources>
            <Style TargetType="Button">
                <Setter Property="Foreground" Value="{StaticResource Text}"/>
                <Setter Property="FontSize" Value="20"/>
                <Setter Property="FontWeight" Value="Bold"/>
                <Setter Property="HorizontalAlignment" Value="Right" />
            </Style>
        </Grid.Resources>
        <Frame x:Name="RegContentFrame" Grid.Column="2" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
